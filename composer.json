{
    "name": "silverstripe/orm",
    "description": "PoC of a Silverstripe ORM package, split out of core. This will be deleted.",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Claust",
    "authors": [
        {
            "name": "Sam Minnee",
            "email": "sam@silverstripe.com"
        }
    ],
    "require":   {
        "silverstripe/core": "*",
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.27",
        "sminnee/phpunit-mock-objects": "^3.4.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\ORM\\": "src/",
            "SilverStripe\\ORM\\Tests\\": "tests/"
        },
        "classmap": [
            "src/_legacy"
        ]
    }
}
